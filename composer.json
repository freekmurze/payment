{
    "name": "spatie/payment",
    "license": "MIT",
    "homepage": "https://github.com/spatie/payment",
    "description": "A Laravel 5 package to verify and accept payments from payment gateways",
    "keywords": [
        "payment",
        "gateway",
        "europabank",
        "e-commerce",
        "webshop",
        "financial",
        "laravel"
    ],
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/support": "~5.1.0|~5.2.0",
        "illuminate/validation": "~5.1.0|~5.2.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Payment\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Payment\\PaymentServiceProvider"
            ],
            "aliases": {
                "Payment": "Spatie\\Payment\\PaymentFacade"
            }
        }
    }
}
